{
  "manifest_version": 3,
  "name": "ProfileBuddy IG Data Extractor",
  "short_name": "PB IG Data Extractor",
  "description": "Instagram leads generator and data extractor for ProfileBuddy",
  "version": "5.0.0",
  "background": {
    "scripts": [
      "assets/js/jquery.js",
      "assets/js/axios.js",
      "background/fireNotification.js",
      "background/main.js",
      "background/changeHeaders.js"
    ],
    "persistent": true
  },
  "externally_connectable": {
    "matches": [
      "https://localhost:3000/*",
      "https://localhost:3001/*",
      "https://app.profilebuddy.io/*"
  ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*",
        "https://localhost:3000/*",
        "https://localhost:3001/*",
        "https://igdashboard.profilebuddy.io/*"
      ],
      "js": [
        "assets/js/jquery.js",
        "assets/js/axios.js",
        "assets/js/sweetAlert.js",

        "models/command.js",

        "content/helper.js",
        "content/main.js"
      ]
    },
    {
      "matches": [
        "https://localhost:3000/maindashboard",
        "https://app.profilebuddy.io/maindashboard"
      ],
      "js": [
        "content/listenToButtonClick.js"
      ]
    }
  ],
  "default_locale":  "en",
  "browser_action": {
    "default_icon": "assets/img/igbuddy_masc0t.png",
    "default_title": "ProfileBuddy - IG Data Scraper"
  },
  "icons": {
    "48": "assets/img/icon-48.png",
    "128": "assets/img/icon-128.png"
  },
  "permissions": [
    "downloads",
    "storage",
    "https://i.instagram.com/*",
    "https://data.profilebud.com/",
    "https://ht.profilebud.com/",
    "https://sdt.profilebud.com/",
    "https://api.profilemate.com/*",
    "https://localhost:3000/*",
    "https://localhost:3001/*",
    "https://app.profilebuddy.io/*",
    "unlimitedStorage",
    "webRequestBlocking",
    "webRequest",
    "scripting",
    "tabs",
    "activeTab",
    "https://www.google-analytics.com/",
    "https://*.cdninstagram.com/*",
    "https://*.cdninstagram.net/*",
    "https://*.fna.fbcdn.net/*"
  ],
  "web_accessible_resources": [
    "inject/inject_script.js",
    "static/js/runtime-main.b1d701af.js",
    "static/js/2.606c0818.chunk.js",
    "static/js/main.34e2022d.chunk.js"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com 'sha256-Kt/s4X/Ekpk2yCCQ5eypl8O9wMhloX8DOdDsVw82XTg=' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU=' 'sha256-+VeZEl5AsVzOPcBzLgETuMOgz2Bc+ruZY5sKtogXQPs=' 'sha256-e5SFxFWkKovG40ixRdeHYo67IkAhU+sftPIwTEmfH3k=' 'sha256-TrJA1SgixVdnS2eZ6Otoxli+j62QNKMfThZzrnMiwIE='; object-src 'self';"
}
